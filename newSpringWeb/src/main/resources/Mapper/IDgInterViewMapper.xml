<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.demo.model.Mapper.IDgPositionMapper">
    <resultMap id="BaseResultMap" type="com.demo.pojo.Interview">
        <id column="did" property="did"/>
        <result column="intype" property="intype"/>
        <result column="intime" property="intime"/>
        <result column="inaddress" property="inaddress"/>
        <result column="dname" property="dname"/>
        <result column="dname" property="dname"/>
        <association property="posi" javaType="com.demo.pojo.Position">
            <id column="pid" property="pid"/>
            <result column="poname" property="poname"/>
            <result column="ptype" property="ptype"/>
            <association property="dept" javaType="com.demo.pojo.Dept">
                <id column="did" property="did"/>
                <result column="dname" property="dname"/>
            </association>
        </association>
    </resultMap>
    <!--根据简历新增面试-->
    <insert id="insertInterView">
        insert into interview value(null,intype=#{intype},intime=#{intime},inaddress=#{inaddress},rsid=#{rsid})
    </insert>

</mapper>